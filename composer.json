{
    "name": "rawphp/raw-request",
    "type": "library",
    "description": "RawRequest is a simple request class used by RawPHP framework and other applications and provides a useful interface to the HTTP request.",
    "keywords": [ "http", "request" ],
    "homepage": "http://rawphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Kaczocha",
            "email": "tom@rawphp.org"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.5",
        "rawphp/raw-code-standards": "1.*@dev",
		"satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RawPHP\\RawRequest\\": "src/RawPHP/RawRequest/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
